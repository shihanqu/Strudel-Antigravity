const scale = cat('C3 dorian','Bb2 major').slow(4);
stack(
  n("2*4").add(12)
  .off(1/8, add("2")).fast(2)
  .add("<0 1 2 1>").hush(),
  n("<0 1 2 3>(3,8,2)")
  .off(1/4, add("2,4")),
  n("<0 4>(5,8,-1)").sub(7)
)
  .scale(scale)
  .gain(".6 .7".fast(4))
  .legato(2)
  .add(note(5))
  .piano()
  .mul(gain(.8))
  .slow(2)
  .pianoroll({maxMidi:100,minMidi:20})
